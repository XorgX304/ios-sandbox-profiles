(version 1)
(allow default)
(deny file-read*
	(subpath "/private/var/run/mobile_image_mounter"))
(deny file-write-unlink
	(literal "/private/var/run")
	(literal "/private")
	(literal "/private/var"))
(deny job-creation)
(deny network-outbound
	(literal "/private/var/tmp/launchd/sock")
	(regex #"^/private/tmp/launchd-[0-9]+[.][^/]+/sock$"))
