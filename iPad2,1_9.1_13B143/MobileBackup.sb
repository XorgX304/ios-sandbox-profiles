(version 1)
(allow default)
(allow file-read*
	(subpath "/private/var/run/mobile_image_mounter"))
(allow file-write*)
(allow file-write-create)
(allow file-write-unlink
	(literal "/private/var")
	(literal "/private/var/run")
	(literal "/private"))
(deny job-creation)
(allow network-bind)
(allow network-outbound
	(regex #"^/private/tmp/launchd-[0-9]+\.[^/]+/sock$"))
